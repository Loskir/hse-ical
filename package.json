{
  "name": "hse-ical",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "distribute": "yarn build-ts && yarn run-dist",
    "run-dist": "node dist/index.js",
    "develop": "concurrently -k -i -p \"[{name}]\" -n \"Node,TypeScript\" -c \"yellow.bold,cyan.bold\" \"yarn watch-js\" \"yarn watch-ts\"",
    "build-ts": "yarn tsc",
    "watch-ts": "yarn tsc -w",
    "watch-js": "nodemon dist/index.js"
  },
  "dependencies": {
    "dotenv": "^14.2.0",
    "got": "11",
    "ical-generator": "^3.2.0",
    "luxon": "^2.3.0"
  },
  "devDependencies": {
    "@types/luxon": "^2.0.9",
    "@types/node": "^17.0.9",
    "concurrently": "^7.0.0",
    "dayjs": "^1.10.7",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.34",
    "nodemon": "^2.0.15",
    "rrule": "^2.6.8",
    "typescript": "^4.5.4"
  }
}
